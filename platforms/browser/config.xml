<?xml version='1.0' encoding='utf-8'?>
<widget id="io.ecossis.doctum" version="2.5.6" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.1" xmlns:gap="http://phonegap.com/ns/1.1">
    <name>Ecossistema Doctum 2.0</name>
    <description>
        Aplicativo para alunos Doctum EAD. Esse app foi desenvolvido para os alunos Doctum nas seguintes modalidades:
        - Graduação EAD
        - Cursos Livres
    </description>
    <author email="contato@doctum.edu.br" href="http://www.doctum.edu.br">
        Equipe de Desenvolvimento Mobile - Doctum
    </author>
    <gap:plugin name="onesignal-cordova-plugin" source="npm" spec="2.11.0" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="android-minSdkVersion" value="27" />
    <preference name="Orientation" value="portrait" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarBackgroundColor" value="#005aaa" />
</widget>
